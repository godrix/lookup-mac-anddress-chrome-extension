{
  "manifest_version": 3,
  "name": "Lookup MAC Address",
  "description": "Find the vendor / manufacturer of a device by its MAC Address with our lookup tool",
  "icons": {
          "128": "icon128.png" },
  "version": "2.4",
  "permissions":[
    "contextMenus",
    "notifications"
  ],
  "action":{
    "default_popup":"popup.html"
  },
  "background":{
    "service_worker":"js/eventPage.js"
  },
  "content_scripts":[
    {
    "matches":["<all_urls>"],
    "js":[
      "js/main.js"
    ],
    "run_at": "document_end",
    "persistent": false
    }
  ]

}